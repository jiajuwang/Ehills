<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.BiddingController">
   <center>
      <ScrollPane fx:id="ScrollPane" prefHeight="272.0" prefWidth="600.0" BorderPane.alignment="CENTER">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="445.0" prefWidth="581.0" />
        </content>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </ScrollPane>
   </center>
   <top>
      <TitledPane animated="false" prefHeight="24.0" prefWidth="600.0" text="Welcome to Bidding" BorderPane.alignment="CENTER">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </TitledPane>
   </top>
   <bottom>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="result" text="Item index" />
            <TextField fx:id="searchField" promptText="Search item:" />
            <Button fx:id="searchButton" mnemonicParsing="false" text="Search" />
            <Button fx:id="history" mnemonicParsing="false" text="History" />
            <Button fx:id="quit" mnemonicParsing="false" text="Quit" />
         </children>
      </HBox>
   </bottom>
</BorderPane>